<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.devMode" value="true" />
    <package name="com.diworksdev.webproj5.action" extends="struts-default">

         <action name="WelcomeColor" class="com.diworksdev.webproj5.action.WelcomeColor" method="execute">
            <result name="success">WelcomeColor.jsp</result>
        </action>

        <action name="LoginColor" class="com.diworksdev.webproj5.action.LoginColor" method="execute">
            <result name="success">login.jsp</result>
            <result name="error">loginError.jsp</result>
        </action>

    </package>
</struts>